    <xs:element name="agreement">
        <xs:annotation>
            <xs:documentation>Root element for the federation agreement.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:metadata" maxOccurs="1"/>
                <xs:element ref="fedagree:design" maxOccurs="1"/>
                <xs:element ref="fedagree:execution" maxOccurs="1"/>
                <xs:element ref="fedagree:management" maxOccurs="1"/>
                <xs:element ref="fedagree:data" maxOccurs="1"/>
                <xs:element ref="fedagree:infrastructure" maxOccurs="1"/>
                <xs:element ref="fedagree:modeling" maxOccurs="1"/>
                <xs:element ref="fedagree:variances" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attribute name="federationName" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="metadata">
        <xs:annotation>
            <xs:documentation>
                Information about the federation agreements document itself.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:identification" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="fedagree:federationExecutions" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:referencedDocs" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:addressBook" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:previousAgreements" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:glossary" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:changelog" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="identification">
        <xs:annotation>
            <xs:documentation>
                Identifies this document; version number and name if applicable. Makes extensive use of the MSC-DMS metadata
                elements, making this the most important element relative to registering the federation agreements document
                in repositories based upon MSC-DMS markup.                
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:Title" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="fedagree:Version" minOccurs="1" maxOccurs="1"/>
                <xs:element name="modificationDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
                <xs:element name="authors" type="fedagree:pocListType" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="fedagree:rights" minOccurs="0" maxOccurs="1"/>
                <xs:element name="notes" type="xs:string" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="federationExecutions">
        <xs:annotation>
            <xs:documentation>
                Defines the federation execution(s) to which the document applies.    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:federationExecution" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="fedagree:remoteDocumentRef" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="federationExecution">
        <xs:annotation>
            <xs:documentation>
                The actual operation, over time, of a set of joined federates that are interconnected by middleware.    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="purpose" type="xs:string"/>
                <xs:element name="applicationDomain" type="fedagree:applicationDomainTypeUnion"
                    minOccurs="0"/>
                <xs:element name="limitations" type="xs:string" minOccurs="0"/>
                <xs:element name="date" type="xs:date" minOccurs="0"/>
                <xs:element name="description" type="xs:string"/>
                <xs:element name="language" type="fedagree:languageType" minOccurs="0"/>
                <xs:element name="capabilities" type="xs:string" minOccurs="0"/>
                <xs:element ref="fedagree:Releasability" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            Releasability designation of federation agreements pertaining to the federation execution.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="fedagree:Security" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            Security classification at which the federation is executed.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="pocs" type="fedagree:pocListType"/>
                <xs:element name="contract" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="executionName" type="xs:string"/>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="referencedDocs">
        <xs:annotation>
            <xs:documentation>
                Location and version of documents referenced in other agreements.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:remoteDocument" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="addressBook">
        <xs:annotation>
            <xs:documentation>
                People and organizations used as points of contact in other agreements. Any POCs listed throughout the rest
                of schema must have an associated address book entry even though the addressBook itself is an optional element.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="people" minOccurs="1" maxOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="fedagree:person" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="organizations" minOccurs="1" maxOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="fedagree:organization" minOccurs="0"
                                maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="previousAgreements">
        <xs:annotation>
            <xs:documentation>
                Prior agreements used for reference during simulation design and development; 
                these may be reused or modified as necessary.    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="fedagree:remoteDocumentRef" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="glossary">
        <xs:annotation>
            <xs:documentation>
                Terminology, Acronyms and definitions.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="definition" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>
                            Term being defined goes in the "term" attribute while the definition goes
                            in the tag body.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attribute name="term" type="xs:string" use="required"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attributeGroup ref="fedagree:common"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="changelog">
        <xs:annotation>
            <xs:documentation>
                A log of changes to the federation agreements document.    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="version" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="changes" type="fedagree:complexDescription"/>
                        </xs:sequence>
                        <xs:attribute name="versionNumber" type="xs:string" use="required"/>
                        <xs:attribute name="modificationDate" type="xs:dateTime" use="optional"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

